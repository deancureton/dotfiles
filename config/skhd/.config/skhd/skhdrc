# make floating window fill screen
ctrl + alt - return : yabai -m window --grid 1:1:0:0:1:1

# float / unfloat window and center on screen
alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

# switch to space
alt - 1 : yabai -m space --focus 1
alt - 2 : yabai -m space --focus 2
alt - 3 : yabai -m space --focus 3
alt - 4 : yabai -m space --focus 4
alt - 5 : yabai -m space --focus 5

# switch to next/previous space
ctrl - right : yabai -m space --focus next
ctrl - left : yabai -m space --focus prev

# swap window to east/west/north/south
ctrl + alt + cmd - right : yabai -m window --swap east
ctrl + alt + cmd - left : yabai -m window --swap west
ctrl + alt + cmd - up : yabai -m window --swap north
ctrl + alt + cmd - down : yabai -m window --swap south

# switch window to next/previous space
ctrl + alt - right : yabai -m window --space next; yabai -m space --focus next; yabai -m window --focus last
ctrl + alt - left : yabai -m window --space prev; yabai -m space --focus prev; yabai -m window --focus last
