            "\\usepackage[utf8]{inputenc}",
            "\\usepackage[sexy]{evan}",
            "",
            "\\begin{document}",
            "",
            "\\title{$1}",
            "\\subtitle{$2}",
            "\\author{Dean Cureton}",
            "\\date{\\today}",
            "",
            "\\maketitle",
            "",
            "$0",
            "",
            "\\end{document}",
        ],
        "description": "evan.sty plugin"
    },
