{
    "New Document": {
        "prefix": "newdoc",
        "body": [
            "\\documentclass{article}",
            "\\usepackage[utf8]{inputenc}",
            "\\usepackage{amsmath}",
            "\\usepackage{amsfonts}",
            "\\usepackage{amssymb}",
            "\\usepackage{amsthm}",
            "\\usepackage{thmtools}",
            "\\usepackage{graphicx}",
            "\\usepackage{tikz}",
            "\\usepackage{hyperref}",
            "\\usepackage{listings}",
            "\\usepackage{xcolor}",
            "",
            "\\title{$1}",
            "\\author{Dean Cureton}",
            "\\date{\\today}",
            "",
            "\\begin{document}",
            "",
            "\\maketitle",
            "",
            "$0",
            "",
            "\\end{document}",
        ],
        "description": "regular boilerplate"
    },
    "New Sexy Document": {
        "prefix": "newsexy",
